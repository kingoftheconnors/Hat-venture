[sub_resource type="GDScript" id=5]
script/source = "extends AnimationPlayer

signal hat_kid_fell
signal hat_fall_finished

func animate(animation_name : String):
	play(animation_name)

var hat_fall_started := false
func _on_Player_landed_on_ground():
	if current_animation == \"sleep\":
		play(\"hat_fall\")
		hat_fall_started = true
		emit_signal(\"hat_kid_fell\")

func is_hat_fall_finished():
	emit_signal(\"hat_fall_finished\")
	return hat_fall_started and current_animation != \"hat_fall\"
"

