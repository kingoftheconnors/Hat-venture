[application]

config/name="Hat- Venture"
run/main_scene="res://title-screen/logo.tscn"
config/icon="res://icon.png"

[autoload]

Gui="*res://gui/gui.tscn"
Constants="*res://constants/GameConstants.tscn"
PlayerGameManager="*res://common/PlayerGameManager.tscn"
SoundSystem="*res://constants/SoundSystem.tscn"
ggsManager="*res://addons/GGS/src/ggs_manager.gd"
SaveSystem="*res://constants/SaveSystem.gd"
IntegerResolutionHandler="*res://addons/integer_resolution_handler/integer_resolution_handler.gd"
SettingsUtils="*res://constants/SettingsUtils.gd"

[debug]

settings/fps/force_fps=60

[display]

window/integer_resolution_handler/base_width=426
window/integer_resolution_handler/base_height=256
window/size/width=426
window/size/height=256
window/size/resizable=false
window/size/test_width=426
window/size/test_height=256
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "res://addons/GGS/plugin.cfg", "res://addons/integer_resolution_handler/plugin.cfg" )

